{
  "name": "@logbun/db",
  "version": "1.0.0",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "private": "true",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "generate": "pnpm with-env drizzle-kit generate:pg",
