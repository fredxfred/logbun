    "@logbun/server-utils": "workspace:^",
    "drizzle-orm": "^0.29.1",
    "pg": "^8.11.3",
    "postgres": "^3.4.3"
  },
  "devDependencies": {
    "dotenv-cli": "^7.3.0",
    "drizzle-kit": "^0.20.6",
    "tsup": "^8.0.1",
    "tsx": "^4.6.2",
    "typescript": "^5.2.2"
  }
}
